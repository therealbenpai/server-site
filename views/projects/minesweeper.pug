    script(type="text/javascript" src="https://code.jquery.com/jquery-3.7.1.slim.min.js")
    script.
      function numSeg(Y) {
        Y = Math.min(Y, 999);
        return (Y < 0) ? ["-", Math.floor((-Y % 100) / 10), -Y % 10] : [Math.floor(Y / 100), Math.floor((Y % 100) / 10), Y % 10]
      }

      class squareData {
        constructor(ad, Z, parent) {
          this.rows = parent.rows;
          this.cols = parent.cols;
          this.mines = parent.mines;
          this.zoomlvl = parent.zoomlvl;
          this.I = parent.I;
          this.parent = parent;
          this.ad = ad;
          this.Z = Z;
          this.ab = 0;
          this.aa = false;
          this.Y = false;
          this.ac = false;
        }
        addToValue(ae) {
          this.ab += ae
        };
        isMine() {
          return this.ab < 0
        };
        isFlagged() {
          return this.aa
        };
        isMarked() {
          return this.Y
        };
        isRevealed() {
          return this.ac
        };
        isHidden() {
          return this.ad < 1 || this.ad > this.rows || this.Z < 1 || this.Z > this.cols
        };
        getRow() {
          return this.ad
        };
        getCol() {
          return this.Z
        };
        getValue() {
          return this.ab
