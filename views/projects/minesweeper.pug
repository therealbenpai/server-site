              }
            }
            ac = (p.squareTest(af)) ? af : undefined
          }

          /**
          * 
          * @param {TouchEvent} ag
          * Changes the face upon touching it
          */
          function faceSetter(ag) {
            if (ag.type === "touchmove") return
            let af = eleSelector(ag);
            document.getElementById("face").className = (af.id == "face") ? "facepressed" : "facesmile"
          }

          /**
          * @param {TouchEvent} af Triggering Event
          * @returns {HTMLElement}
          * @description
          * Grabs the element the user is touching
          */
          function eleSelector(af) {
            if (af.type === "touchmove" || af.type === "touchend") {
              let ag = af.originalEvent.changedTouches[0];
              return document.elementFromPoint(ag.clientX, ag.clientY)
            } else return af.target
          }

          p.k = false

          $(document).bind("gesturestart", function (af) {
            p.C = true;
            scrollManager()
          });

          $(document).bind("gestureend", function (af) {
            p.C = false
          });

          $(document).bind("scroll", scrollManager);

          function scrollManager() {
            if (!p.d) return
            p.d = null;
            if (ac) {
              classSetter2(p.c[ac.id], "square blank", "square question");
              ac = undefined
            }
            if (!p.L) document.getElementById("face").className = "facesmile"
          }

          $(document).mousedown(function (ag) {
            let af = mousePressTest(ag);
            p.e = af.left || p.e;
            p.v = af.right || p.v;
            if (ag.ctrlKey && p.squareTest(ag.target) && !L) {
              if (p.er) return;
              p.c[ag.target.id].flag();
              p.isMouseDownForCtrlClick = true
            } else {
              if (p.e) {
                if (p.squareTest(ag.target) && !p.L) {
                  ag.preventDefault();
                  $(document).bind("mousemove", markFunc1);
                  document.getElementById("face").className = "faceooh";
                  ac = undefined;
                  markFunc1(ag)
                } else {
                  if (ag.target.id == "face") {
                    ag.preventDefault();
                    aa = true;
                    $(document).bind("mousemove", faceSetter);
                    document.getElementById("face").className = "facepressed"
                  }
                }
              } else {
                if (p.v) {
                  if (p.squareTest(ag.target) && !p.L) {
                    if (p.er) return;
                    p.c[ag.target.id].flag()
                  }
                  return false
                }
              }
            }
          });

          $(document).mouseup(function (ai) {
            let af = p.mousePressTest(ai);
            let ah;
            let ag;
            if (p.isMouseDownForCtrlClick) {
              p.e = false;
              p.v = false;
              p.isMouseDownForCtrlClick = false;
              return
            }
            if (p.er) {
              if (!(ai.target.id == "face" && aa)) return
              p.newGame()
              aa = false
            }
            if (af.left) {
              p.e = false;
              $(document).unbind("mousemove", markFunc1).unbind("mousemove", faceSetter);
              if (aa || !p.L) document.getElementById("face").className = "facesmile"
              if (p.squareTest(ai.target) && !p.L) {
                ah = p.c[ai.target.id];
                if (p.v) {
                  p.D = true;
                  p.classSetter3(p.c[ai.target.id], "square blank", "square question");
                } else {
                  if (!p.D) {
                    if (!g) {
                      ag = p.checkMineLayer(ah)
                      p.g = true
                    }
                    if (!ah.reveal1()) p.gameDeath([ah])
                  }
                  D = false
                }
              }
            }
            if (af.right) {
              p.v = false;
              if (p.squareTest(ai.target) && !L) {
                if (p.e) {
                  ah = p.c[ai.target.id];
                  p.D = true;
                  p.classSetter3(ah, "square blank", "square question");
                } else p.D = false
                if (!p.L) document.getElementById("face").className = "facesmile"
              }
            }
          });

          $(document).keydown(function (ag) {
            if (ag.which !== 83 && er) {
              ag.preventDefault()
              return
            }
            if (ag.which == 83) {
              ag.preventDefault()
              p.newGame()
            } else if (ag.which == 32) {
              ag.preventDefault()
              square = p.c[p.hoveredSquareId];
              square.flag()
            } else if (ag.which == 68) p.gameDeath([p.c[p.hoveredSquareId]])
            else if (ag.which == 70) document.body.requestFullscreen()
            else if (ag.key == "~") p.gameWin()
            else return
          });

          $("#game").mouseover(function (af) {
            if (squareTest(af.target)) p.hoveredSquareId = af.target.id
          });

          $("#game").mouseout(function (af) {
            if (squareTest(af.target)) {
              if (p.hoveredSquareId = af.target.id) {
                p.hoveredSquareId = ""
              }
            }
          })

          $(document).contextmenu(function (event) {
            event.preventDefault()
          })
        }
      }
